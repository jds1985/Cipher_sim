{
  "version": 2,
  "functions": {
    "api/**/*.cjs": {
      "runtime": "nodejs18.x",
      "memory": 300,
      "maxDuration": 30
    },
    "api/**/*.js": {
      "runtime": "nodejs18.x",
      "memory": 300,
      "maxDuration": 30
    }
  },
  "buildCommand": "npm install && npm run build",
  "installCommand": "npm install",
  "devCommand": "next dev",
  "framework": "nextjs",
  "env": {
    "NODE_ENV": "production",
    "NODE_VERSION": "18"
  }
}
